{
  "name": "redux-fly",
  "version": "0.0.1-alpha.11",
  "description": "Easy to manage redux state",
  "browser": "dist/redux-fly.js",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "files": [
    "dist",
    "lib",
    "es",
    "src"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:commonjs && npm run build:es && npm run build:umd && npm run build:umd:min",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build:umd": "cross-env BABEL_ENV=commonjs NODE_ENV=development webpack src/index.js dist/redux-fly.js",
    "build:umd:min": "cross-env BABEL_ENV=commonjs NODE_ENV=production webpack src/index.js dist/redux-fly.min.js",
    "clean": "rimraf lib es",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "flow": "flow; test $? -eq 0 -o $? -eq 2"
  },
  "author": "Efremov Alexander <mr.efrem@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-eslint": "^7.0.0",
    "babel-jest": "^16.0.0",
    "babel-loader": "^6.2.7",
    "babel-preset-latest": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.13.0",
    "cross-env": "^3.0.0",
    "enzyme": "^2.5.1",
    "eslint": "^3.5.0",
    "eslint-plugin-import": "^1.0.0",
    "eslint-plugin-react": "^6.3.0",
    "jest-cli": "^16.0.0",
    "react": "^15.4.0-rc.4",
    "react-addons-test-utils": "^15.4.0-rc.4",
    "react-dom": "^15.4.0-rc.4",
    "react-redux": "^4.4.5",
    "react-test-renderer": "^15.4.0-rc.4",
    "redux": "^3.6.0",
    "rimraf": "^2.5.4",
    "webpack": "^1.13.3",
    "webpack-bundle-analyzer": "^1.4.1"
  },
  "dependencies": {
    "lodash": "^4.16.1"
  },
  "peerDependencies": {
    "react": "15.x",
    "redux": "3.x",
    "react-redux": "4.x"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFiles": [
      "./setup.tests.js"
    ]
  }
}
